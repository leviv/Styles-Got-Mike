{
  "manifest_version": 2,
  "name": "Style's Got Mike",
  "version": "1.1",
  "content_scripts": [{
    "css": ["test.css"],
    "js": ["main.js"],
    "matches": ["<all_urls>"]
  }],
    "browser_action": {
		"default_popup": "popup.html"
	},"icons": { "16": "icon16.png",
			  "48": "icon48.png",
			  "128": "icon128.png" },
	"permissions": [
		"activeTab",
        "storage"
	]
    
}

